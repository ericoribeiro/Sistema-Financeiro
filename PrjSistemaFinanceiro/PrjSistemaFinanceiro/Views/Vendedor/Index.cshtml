@model List<Model.Entity.Vendedor>
@{
    ViewBag.Title = "Index";
}

<a href="">Relatórios</a>
<h2 class="text-center text-success">Lista de Clientes</h2>
<div class="well well-sm">
    <a class="btn btn-primary" href="">Novo</a>
    <a class="btn btn-info" href="">Buscar</a>
</div>
<table class="table table-hover table-striped">
    <thead>
        <tr class="bg-primary">
            <th>Código</th>
            <th>Nome</th>
            <th>CPF</th>
            <th>Telefone</th>
            <th colspan="3">Operações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var objVendedor in Model)
        {
            <tr>

                <td>@objVendedor.IdVendedor</td>
                <td>@objVendedor.Nome</td>
                <td>@objVendedor.CPF</td>
                <td>@objVendedor.Telefone</td>
                <td><a class="btn btn-success" href="#"><span class="glyphicon glyphicon-eye-open"></span></a></td>
                <td><a class="btn btn-info" href="#"><span class="fa fa-edit"></span></a></td>
                <td><a class="btn btn-danger" href="#"><span class="fa fa-book"></span></a></td>
            </tr>
        }
    </tbody>

</table>

